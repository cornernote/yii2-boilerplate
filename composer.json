{
  "name": "cornernote/yii2-boilerplate",
  "description": "12factor Application Template for Yii 2.0 Framework",
  "keywords": [
    "phundament",
    "yii2",
    "app",
    "framework",
    "application",
    "template",
    "boilerplate",
    "vagrant",
    "docker",
    "fig",
    "12factor",
    "dotenv"
  ],
  "homepage": "http://www.phundament.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Tobias Munk",
      "email": "schmunk@usrbin.de",
      "homepage": "http://www.phundament.com"
    }
  ],
  "support": {
    "issues": "https://github.com/phundament/app/issues?state=open",
    "source": "https://github.com/phundament/app"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "dektrium/yii2-user": "0.9.*",
    "dektrium/yii2-rbac": "dev-master",
    "dmstr/yii2-adminlte-asset": "2.*@dev",
    "dmstr/yii2-app-command": "@stable",
    "dmstr/yii2-log": "@stable",
    "dmstr/yii2-migrate-command": "@stable",
    "mikehaertl/php-shellcommand": "1.*",
    "vlucas/phpdotenv": "1.1.*",
    "yiisoft/yii2": "2.0.*",
    "yiisoft/yii2-bootstrap": "2.*",
    "yiisoft/yii2-composer": "*",
    "yiisoft/yii2-swiftmailer": "2.*",
    "bower-asset/jquery": "@stable",
    "kartik-v/yii2-widget-select2": "*",
    "vova07/yii2-console-runner-extension": "*",
    "raoul2000/yii2-workflow": "*",
    "zhuravljov/yii2-datetime-widgets" : "*",
    "cornernote/yii2-ace-admin": "dev-master",
    "cornernote/yii2-gii-tools": "dev-master",
    "cornernote/yii2-return-url": "dev-master",
    "cornernote/yii2-audit-module": "dev-master",
    "cornernote/yii2-softdelete": "dev-master",
    "cornernote/yii2-linkall-behavior": "dev-master",
    "svyatov/yii-shortcut": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-apidoc": "2.*",
    "yiisoft/yii2-codeception": "2.*",
    "yiisoft/yii2-coding-standards": "2.*",
    "yiisoft/yii2-debug": "2.*",
    "yiisoft/yii2-faker": "2.*",
    "yiisoft/yii2-gii": "2.*",
    "schmunk42/yii2-giiant": "dev-master",
    "cebe/markdown-latex": "dev-master"
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "config": {
    "process-timeout": 1800
  }
}
